<%= render 'navbar' %>
<div class="container mt-3">
  <div class="row">
    <div class="col-12">
      <h1>All our flats (<%= @flats.length %>)</h1>
      <hr>
    </div>
  </div>
  <div class="row">
      <% @flats.each do |flat| %>
        <div class="col-md-4 mb-4">
          <%= render 'card', flat: flat %>
        </div>
      <% end %>
  </div>
</div>
